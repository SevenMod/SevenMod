<?xml version="1.0" encoding="utf-8" ?>
<Phrases>
  <Phrase Name="Yes">
    <en>Yes</en>
  </Phrase>
  <Phrase Name="No">
    <en>No</en>
  </Phrase>
  <Phrase Name="Vote question">
    <Format>{1:s}</Format>
    <en>[Vote] {1}</en>
  </Phrase>
  <Phrase Name="Vote option">
    <Format>{1:d},{2:s}</Format>
    <en>[Option] /{1}: {2}</en>
  </Phrase>
  <Phrase Name="Vote Yes Or No">
    <en>[Vote] Enter /yes or /no into chat to cast your vote</en>
  </Phrase>
  <Phrase Name="Vote Number">
    <en>[Vote] Enter /# into chat to cast your vote</en>
  </Phrase>
  <Phrase Name="Voted For">
    <Format>{1:N},{2:s}</Format>
    <en>[Vote] {1} voted for {2}</en>
  </Phrase>
  <Phrase Name="Changed Vote">
    <Format>{1:N},{2:s}</Format>
    <en>[Vote] {1} changed their vote to {2}</en>
  </Phrase>
  <Phrase Name="You Voted">
    <Format>{1:s}</Format>
    <en>[Vote] You voted for {1}</en>
  </Phrase>
  <Phrase Name="Vote Succeeded">
    <Format>{1:p2}</Format>
    <en>[Vote] Vote succeeded with {1} of the vote.</en>
  </Phrase>
  <Phrase Name="Vote Failed">
    <Format>{1:p2}</Format>
    <en>[Vote] Vote failed with {1} of the vote.</en>
  </Phrase>
</Phrases>